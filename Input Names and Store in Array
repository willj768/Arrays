import java.util.Scanner;

public class Main
{
    public static void main(String[] args) {
        
        Scanner input = new Scanner(System.in);
        
        String names[] = new String[7];
        
        for(int i = 0; i <= 6; i++) {
            System.out.println("Enter a name");
            String name = input.nextLine();
            names[i] = name;
        }
    }
}
