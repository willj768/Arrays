import java.util.Scanner;

public class Main
{
    public static void main(String[] args) {
        
        Scanner input = new Scanner(System.in);
        
        int integers[] = new int[10];
        
        for(int i = 0; i <= 9; i++) {
            System.out.println("Enter a number");
            int integer= input.nextInt();
            
            integers[i] = integer;
        }
    }
}
